{
  "name": "url2pdf",
  "version": "1.0.0",
  "description": "A url to PDF micro service.",
  "author": "Naomi Hauret",
  "license": "MIT",
  "repository": {
    "type": "git"
  },
  "husky": {
    "hooks": {
      "prepare-commit-msg": "exec < /dev/tty && git cz --hook || true",
      "pre-commit": "lint-staged"
    }
  },
  "lint-staged": {
    "*.{js,ts}": [
      "prettier --write",
      "eslint --fix",
      "git add"
    ]
  },
  "scripts": {
    "prepare": "husky install",
    "build": "npm run test:ci && npm run vercel-build ",
    "start": "vercel dev",
    "lint": "eslint .",
    "test": "jest",
    "test:ci": "jest --ci",
    "cm": "cz"
  },
  "dependencies": {
    "chrome-aws-lambda": "^8.0.2",
    "puppeteer-core": "^8.0.0",
    "url-exist": "2.0.2"
  },
  "devDependencies": {
    "@babel/core": "^7.13.16",
    "@babel/preset-env": "^7.13.15",
    "babel-jest": "^26.6.3",
    "eslint": "^7.25.0",
    "cz-conventional-changelog": "^3.3.0",
    "eslint-config-prettier": "^8.3.0",
    "eslint-plugin-prettier": "^3.4.0",
    "husky": "^6.0.0",
    "jest": "^26.6.3",
    "lint-staged": "^10.5.4",
    "node-mocks-http": "^1.10.1",
    "prettier": "^2.2.1",
    "puppeteer": "^9.0.0"
  },
  "jest": {
    "moduleNameMapper": {
      "@api(.*)$": "<rootDir>/api/$1"
    }
  },
  "config": {
    "commitizen": {
      "path": "./node_modules/cz-conventional-changelog"
    }
  }
}
